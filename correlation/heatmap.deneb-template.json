      "dataPointLimit": 50
    },
    "information": {
      "name": "Correlation - Heatmap",
      "description": "[No Description Provided]",
      "author": "Andrzej Leszkiewicz",
      "uuid": "99520476-f753-4c6c-8098-bc5efed33ed1",
