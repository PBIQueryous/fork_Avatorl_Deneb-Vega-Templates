      "previewImageBase64PNG": "data:image/png;base64,"
    },
    "dataset": [
      {
        "key": "__0__",
        "name": "X",
        "description": "",
        "type": "text",
        "kind": "column"
      },
      {
        "key": "__1__",
        "name": "Y",
        "description": "",
        "type": "text",
