{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "usermeta": {
    "deneb": {
      "build": "1.5.1.0",
      "metaVersion": 1,
      "provider": "vega",
      "providerVersion": "5.23.0"
    },
    "interactivity": {
      "tooltip": true,
      "contextMenu": true,
      "selection": true,
      "highlight": true,
      "dataPointLimit": 50
    },
    "information": {
      "name": "Deviation - Surplus/Deficit Filled Line Chart",
      "description": "To show balance against a baseline or between 2 series.",
      "author": "Andrzej Leszkiewicz",
      "uuid": "42352bed-456c-4546-ae07-aef3509fcae8",
      "generated": "2023-09-15T11:46:44.949Z",
      "previewImageBase64PNG": "data:image/png;base64,"
    },
    "dataset": [
      {
        "key": "__0__",
        "name": "Date",
        "description": "date-time - Axis X",
        "type": "dateTime",
        "kind": "column"
      },
      {
        "key": "__1__",
        "name": "Difference",
        "description": "variance (positive/negative values) - Axis Y",
        "type": "numeric",
        "kind": "column"
      }
    ],
    "Project": "Deneb Vega Templates by Andrzej Leszkiewicz",
    "Github": "https://github.com/avatorl/Deneb-Vega-Templates/",
    "Author": "Andrzej Leszkewicz",
    "License": "CC BY 4.0 (Share and Adapt with Attribution)",
    "Inspired By (chart types)": "FT Visual Vocabulary (https://ft.com/vocabulary)"
  },
  "config": {},
  "description": "Deviation - Surplus/Deficit Filled Line Chart. To show balance against a baseline or between 2 series.",
  "padding": 15,
  "autosize": "fit",
  "background": "white",
  "signals": [
    {
      "name": "width",
      "description": "Responsive. Based on container size.",
      "value": 400,
      "update": "containerSize()[0]-30"
    },
    {
      "name": "height",
      "description": "Responsive. Based on container size.",
      "value": 400,
      "update": "containerSize()[1]-30"
    },
    {
      "name": "titleText",
      "value": "Surplus/Deficit Filled Line Chart"
    },
    {
      "name": "subTitleText",
      "value": "To show balance against a baseline or between 2 series"
    },
    {
      "name": "barColor1",
      "init": "pbiColor(0)"
    },
    {
      "name": "barColor2",
      "init": "pbiColor(2)"
    },
    {
      "name": "titleColor",
      "value": "#333333"
    },
    {
      "name": "labelColor",
      "value": "#666666"
    },
    {
      "name": "titleFontSize",
      "value": 24
    },
    {
      "name": "subtitleFontSize",
      "value": 16
    },
    {
      "name": "labelFontSize",
      "value": 16
    },
    {
      "name": "titleOffset",
      "description": "Blank space between the subtitle and the chart, px",
      "value": 20
    },
    {
      "name": "subtitlePadding",
      "description": "Blank space between the title and the subtitle, px",
      "value": 10
    }
  ],
  "data": [
    {
      "name": "dataset",
      "transform": [
        {
          "type": "formula",
          "as": "surplus",
          "expr": "max(datum['__1__'], 0)"
        },
        {
          "type": "formula",
          "as": "deficit",
          "expr": "min(datum['__1__'], 0)"
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "scaleX",
      "type": "time",
      "domain": {
        "data": "dataset",
        "field": "__0__"
      },
      "range": "width"
    },
    {
      "name": "scaleY",
      "type": "linear",
      "domain": {
        "data": "dataset",
        "field": "__1__"
      },
      "range": "height"
    }
  ],
  "title": {
    "text": {"signal": "titleText"},
    "anchor": "start",
    "offset": {"signal": "titleOffset"},
    "color": {"signal": "titleColor"},
    "fontSize": {
      "signal": "titleFontSize"
    },
    "subtitle": {
      "signal": "subTitleText"
    },
    "subtitlePadding": {
      "signal": "subtitlePadding"
    },
    "subtitleFontSize": {
      "signal": "subtitleFontSize"
    }
  },
  "axes": [
    {
      "orient": "left",
      "scale": "scaleY",
      "labels": true,
      "labelFontSize": {
        "signal": "labelFontSize"
      },
      "labelColor": {
        "signal": "labelColor"
      },
      "ticks": true
    },
    {
      "orient": "bottom",
      "scale": "scaleX",
      "labels": true,
      "labelFontSize": {
        "signal": "labelFontSize"
      },
      "labelColor": {
        "signal": "labelColor"
      },
      "labelOverlap": true,
      "ticks": true
    }
  ],
  "marks": [
    {
      "name": "area-surplus",
      "type": "area",
      "from": {"data": "dataset"},
      "encode": {
        "enter": {
          "x": {
            "scale": "scaleX",
            "field": "__0__"
          },
          "y": {
            "scale": "scaleY",
            "field": "surplus"
          },
          "y2": {
            "scale": "scaleY",
            "signal": "0"
          },
          "fill": {
            "signal": "barColor1"
          }
        }
      }
    },
    {
      "name": "area-deficit",
      "type": "area",
      "from": {"data": "dataset"},
      "encode": {
        "enter": {
          "x": {
            "scale": "scaleX",
            "field": "__0__"
          },
          "y": {
            "scale": "scaleY",
            "field": "deficit"
          },
          "y2": {
            "scale": "scaleY",
            "signal": "0"
          },
          "fill": {
            "signal": "barColor2"
          }
        }
      }
    }
  ]
}
